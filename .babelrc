{
  "plugins": [
    [
      "@babel/transform-react-jsx"
    ],
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "isTSX": true,
        "allExtensions": true
      }
    ]
  ]
}
